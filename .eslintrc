{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    // "plugin:react/recommended",
    "plugin:react/jsx-runtime",
    "airbnb"
  ],
  "overrides": [
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
      "react"
  ],
  "rules": {
    "react/react-in-jsx-scope": "off",
    "no-console": "off",
    "react/prop-types": 0,
    "no-underscore-dangle": [
      "error",
      { "allow": ["_id"] }
    ],
    "class-methods-use-this": [
      "error",
      { "exceptMethods": ["#checkOk"] }
    ]
  }
}
